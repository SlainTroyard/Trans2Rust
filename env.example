# CStarX v2.0 Environment Configuration

# Model Configuration
CSTARX_MODEL_PROVIDER=openai
CSTARX_MODEL_NAME=deepseek-chat
CSTARX_API_KEY=sk-xxxxxx
CSTARX_BASE_URL=https://api.deepseek.com
CSTARX_TEMPERATURE=0.7
CSTARX_TOP_P=0.9
CSTARX_MAX_TOKENS=8192
CSTARX_TIMEOUT=60

# Alternative Model Providers
# CSTARX_MODEL_PROVIDER=anthropic
# CSTARX_MODEL_NAME=claude-3-sonnet-20240229
# CSTARX_API_KEY=your_anthropic_api_key_here

# CSTARX_MODEL_PROVIDER=zhipu
# CSTARX_MODEL_NAME=glm-4
# CSTARX_API_KEY=your_zhipu_api_key_here

# Translation Configuration
CSTARX_TRANSLATION_STRATEGY=adaptive
CSTARX_MAX_PARALLEL_WORKERS=5
CSTARX_RETRY_ATTEMPTS=3
CSTARX_ENABLE_MEMORY=true
CSTARX_ENABLE_QUALITY_CHECK=true
CSTARX_ENABLE_TEST_GENERATION=true

# Dependency Analysis
CSTARX_USE_CLANG=true
CSTARX_CLANG_PATH=clang
CSTARX_COMPILE_COMMANDS_PATH=
CSTARX_INCLUDE_PATHS=/usr/include,/usr/local/include
CSTARX_DEFINE_MACROS=
CSTARX_OPTIMIZATION_LEVEL=0

# Output Configuration
CSTARX_OUTPUT_DIR=./Output
CSTARX_PRESERVE_STRUCTURE=true
CSTARX_GENERATE_TESTS=true
CSTARX_GENERATE_DOCS=false
CSTARX_FORMAT_CODE=true

# Logging
CSTARX_LOG_LEVEL=INFO
CSTARX_LOG_FILE=

# Development
CSTARX_DEV_MODE=false
CSTARX_DEBUG=false

# Project Settings
CSTARX_PROJECT_NAME=
CSTARX_PROJECT_PATH=
CSTARX_TARGET_LANGUAGE=rust
